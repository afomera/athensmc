<tr>
  <td>
    <%= link_to user.id,
      admin_user_path(user.id),
      class: "tag-tooltip",
      data: {
        'data-toggle' => "tooltip",
        'data-placement' => "left",
        title: "Tooltip on left"
      } %>
  </td>
  <td>
    <%= image_tag user.minecraft_avatar,
      :height => "64px",
      :class => "tag-tooltip",
      data: {
        'data-toggle' => "tooltip",
        'data-placement' => "left",
        title: user.dashed_uuid
      }  %>
  </td>
  <td><%= link_to user.username, user_path(user.username) %></td>
  <td><%= link_to user.email, admin_user_path(user.id) %> </td>
  <td><%= user.last_sign_in_ip %></td>
  <td><%= local_time_ago(user.last_sign_in_at) if user.last_sign_in_at.present? %> </td>
  <td><% if user.admin? %><span class="label label-danger">Admin</span> <% else %> <span class="label label-green">User</span> <% end %></td>
  <td><%= user.dashed_uuid %></td>
</tr>
