<tr>
  <td><%= map_download.created_at.strftime("%m/%d/%Y") %>
  <td><%= map_download.name %></td>
  <td><%= map_download.description %></td>
  <td><%= link_to 'Download', map_download.link %></td>
  <td>
  <%= link_to edit_admin_download_path(map_download) do %>
    <i class="fa fa-pencil fa-2x"></i>
  <% end %>
  &nbsp;
  <%= link_to admin_download_path(map_download), :method => :delete, data: { confirm: "Are you sure you want to delete this download?" } do %>
    <i class="fa fa-trash-o fa-2x"></i>
  <% end %>
  </td>
</tr>
